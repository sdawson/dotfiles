[user]
	name = Sophie Dawson
	email = sophiecdawson@gmail.com
[mergetool "diffmerge"]
	cmd = diffmerge --merge --result=$MERGED $LOCAL $BASE $REMOTE
	trustExitCode = false
[color]
	diff = auto
	status = auto
	branch = auto
[core]
  whitespace = trailing-space,space-before-tab
  excludesfile = ~/.gitignore
[diff]
  renames = copies
  mnemonicprefix = true
[branch]
  autosetupmerge = true
[push]
  default = tracking
[rerere]
  enabled = 1
[alias]
  ll = log --graph --oneline --decorate --date=short --all --pretty=format:'%ad %h %Cgreen%an %Cred%d %Creset%s'
